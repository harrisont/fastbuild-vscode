{
    "comments": {
        "lineComment": "//"
    },
    "brackets": [
        [ "{", "}" ],
        [ "[", "]" ],
        [ "(", ")" ]
    ],
    "colorizedBracketPairs": [
        [ "{", "}" ],
        [ "[", "]" ],
        [ "(", ")" ]
    ],
    "autoClosingPairs": [
        { "open": "{", "close": "}" },
        { "open": "[", "close": "]" },
        { "open": "(", "close": ")" },
        { "open": "'", "close": "'", "notIn": ["string", "comment"] },
        { "open": "\"", "close": "\"", "notIn": ["string"] },
        { "open": "/*", "close": "*/", "notIn": ["string"] }
    ],
    "autoCloseBefore": "=}]) \n\t",
    "surroundingPairs": [
        [ "{", "}" ],
        [ "[", "]" ],
        [ "(", ")" ],
        [ "'", "'" ],
        [ "\"", "\"" ]
    ],
    "folding": {
        "markers": {
            "start": "^\\s*#if",
            "end": "^\\s*#endif"
        }
    }
}